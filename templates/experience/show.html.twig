{% extends 'base.html.twig' %}

{% block title %}Experience{% endblock %}

{% block body %}
    <h1>Experience</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ experience.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ experience.name }}</td>
            </tr>
            <tr>
                <th>DateStart</th>
                <td>{{ experience.dateStart }}</td>
            </tr>
            <tr>
                <th>DateEnd</th>
                <td>{{ experience.dateEnd }}</td>
            </tr>
            <tr>
                <th>Place</th>
                <td>{{ experience.Place }}</td>
            </tr>
            <tr>
                <th>Text</th>
                <td>{{ experience.Text }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('experience_index') }}">back to list</a>

    <a href="{{ path('experience_edit', {'id': experience.id}) }}">edit</a>

    {{ include('experience/_delete_form.html.twig') }}
{% endblock %}
